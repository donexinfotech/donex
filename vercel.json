{
    "builds": [
      { "src": "Website/build", "use": "@vercel/static-build" },
      { "src": "donex/wsgi.py", "use": "@vercel/python", "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" } }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "donex/wsgi.py" },
      { "src": "/(.*)", "dest": "Website/build/index.html" }
    ],
    "cleanUrls": true
  }
  