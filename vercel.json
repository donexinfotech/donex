{
    "builds": [
      {
        "src": "donex/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "Website/build",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "donex/wsgi.py"
      }
    ],
    "buildCommand": "sh build.sh"
  }